{
  "name": "Train-System",
  "tree": {
    "$className": "DataModel",

    "ReplicatedStorage": {
      "assets": {
        "$className": "Folder",
        "Trains": {
          "$path": "assets/trains"
        }
      },
      "src": {
        "$path": "./src/shared"
      },
      "Packages": {
        "$path": "Packages"
      }
    },

    "ServerScriptService": {
      "src": {
        "$path": "./src/server"
      }
    },

    "ServerStorage": {
      "src": {
        "$path": "./src/storage"
      }
    },

    "Players": {
      "$properties": {
        "CharacterAutoLoads": true
      }
    },

    "StarterPlayer": {
      "StarterPlayerScripts": {
        "src": {
          "$path": "./src/client"
        }
      }
    },

    "Workspace": {
      "Terrain": {
        "$path": "assets/Terrain.rbxm"
      },
      "$properties": {
        "FilteringEnabled": true
      },
      "Map": {
        "$path": "assets/Map"
      }
    },
    "Lighting": {
      "$path": "assets/Lighting.rbxm"
    },
    "SoundService": {
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}